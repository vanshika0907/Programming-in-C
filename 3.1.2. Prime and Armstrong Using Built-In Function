#include <stdio.h>
#include <math.h>

// Function to check if a number is prime
int is_prime(int n) {
	int i;
if (n<= 1)
	return 0; // Not Prime
for (int i = 2; i <= sqrt(n); i++){
	if (n % i == 0)
		return 0; // Divisible, not prime
}
return 1;
}
int is_armstrong(int n){
	int temp, digit, count = 0;
	int sum = 0;

	temp = n;

	while (temp != 0){
		count++;
			temp /= 10;
	}
	temp = n;

	while (temp != 0){
		digit = temp % 10;
		sum += pow(digit, count);
		temp /= 10;
	}
	if (sum == n)
		return 1;
	else
		return 0;
}


int main() {
	int n;
	
	// Input from the user
	scanf("%d", &n);
	
	// Check if the number is prime
	if (is_prime(n))
		printf("%d is prime\n", n);
	else
		printf("%d is not prime\n", n);
	
	// Check if the number is an Armstrong number
	if (is_armstrong(n))
		printf("%d is Armstrong\n", n);
	else
		printf("%d is not Armstrong\n", n);
	
	return 0;
}
