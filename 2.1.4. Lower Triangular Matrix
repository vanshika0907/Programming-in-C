#include <stdio.h>
int main() {
	int row, col, is_lower_triangular_matrix = 1;
	printf("Enter no of rows, columns: ");
	scanf("%d %d", &row, &col);
	int arr[row][col];
	printf("Enter elements of matrix:\n");
	for (int i = 0; i < row; i++){
		for (int j = 0; j < col; j++){ 
		scanf("%d", &arr[i][j]);
		}
	}
if ( !(row == col) ) {
	printf ("Matrix must be square matrix\n");
	return 0;
}
for (int i = 0; i < row; i++) {
	for (int j = i + 1; j < col; j++) {
		if (arr[i][j] != 0) {
			is_lower_triangular_matrix = 0;
			break;
		}
	}
}
if (is_lower_triangular_matrix){
	printf("Lower triangular matrix\n");
} else {
	printf("Not a lower triangular matrix\n");
}
	return 0;
}
